  <table class="table table-striped table-bordered">
  <thead>
    <th>Agent Name</th>
    <th>Address</th>
    <th>Phone</th>
    <th>Email</th>
    <th>Status</th>
  </thead>

  <tbody>
    <% @agents.each do |agent| %>
      <tr>
        <td><%= agent.name %></td>
        <td><%= agent.address %></td>
        <td><%= agent.phone %></td>
        <td><%= agent.email %></td>
        <td><%= agent.status %></td>
      </tr>
    <%end%>
  </tbody>
</table>